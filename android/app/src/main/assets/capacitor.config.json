{
	"appId": "com.capacitorjs.app.testapp",
	"appName": "TESTAPP",
	"webDir": "dist",
	"plugins": {
		"CapacitorCookies": {
			"enabled": true
		},
		"CapacitorHttp": {
			"enabled": true
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_config_sample",
			"iconColor": "#CE0B7C"
		},
		"PushNotifications": {
			"presentationOptions": [
				"alert",
				"sound"
			]
		},
		"plugins": {
